<Project>

    <meta 
        title="ExtensionDiscordRPC"
        package="com.brenninho.extensiondiscordrpc"
        version="1.0.0"
        company="Brenninho123"
    />

    <app main="Main" path="export" file="ExtensionDiscordRPC" />

    <window 
        width="1280"
        height="720"
        fps="60"
        background="#0f0f14"
        hardware="true"
        vsync="true"
        resizable="false"
    />

    <source path="source" />

    <haxelib name="lime" />
    <haxelib name="openfl" />

    <target name="windows" />

    <haxedef name="desktop" />
    <haxedef name="HXCPP_M64" />

</Project>